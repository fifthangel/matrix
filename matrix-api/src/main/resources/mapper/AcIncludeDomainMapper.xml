	<select id="selectById" resultMap="BaseResultMap" parameterType="java.lang.Integer">
		select
			*
		from 
			apicenter.ac_include_domain
		where 
			id = #{id,jdbcType=INTEGER}
	</select>
	
	<!-- 分页数据 -->
	<select id="queryPage" parameterType="com.matrix.pojo.entity.AcIncludeDomain" resultMap="BaseResultMap">
		select 
			* 
		from 
			apicenter.ac_include_domain a
		<where>
			a.flag = 1  
			<if test="domain != null and domain != ''  ">
				and a.domain = #{domain , jdbcType=VARCHAR}
			</if>
			<if test="companyName != null and companyName != ''  ">
				and a.company_name like CONCAT('%' , '${companyName}' , '%' )
			</if> 
			order by a.id asc
		</where>
	</select>
	
